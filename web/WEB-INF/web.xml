<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
    <servlet>
        <servlet-name>geocachingtools</servlet-name>
        <servlet-class>dk.esmann.geocaching.GeocachingToolsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>uploadpq</servlet-name>
        <servlet-class>dk.esmann.geocaching.UploadPocketQueryServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>geocachingtools</servlet-name>
        <url-pattern>/tool</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>uploadpq</servlet-name>
        <url-pattern>/uploadpq</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
